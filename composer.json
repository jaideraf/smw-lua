{
	"name": "mediawiki/semantic-mediawiki-lua",
	"type": "mediawiki-extension",
	"description": "SemanticMediawiki framework to support Lua/Scribunto integration",
	"keywords": [
		"SMW",
		"Semantic MediaWiki",
		"Wiki",
		"MediaWiki",
    "Lua",
    "Scribunto"
	],
	"homepage": "https://github.com/DataValues/Geo",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "James Hong Kong",
			"homepage": "https://semantic-mediawiki.org/wiki/User:MWJames"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"mediawiki/semantic-mediawiki": "dev-master"
	},
	"minimum-stability" : "dev",
	"autoload": {
		"files" : [
			"Lua.php"
		],
		"psr-0": {
			"SMW\\Lua\\": "src"
		}
	}
